<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <data class="TimeviewModelBinding">

        <import type="android.view.View" />

        <variable
            name="model"
            type="fudex.bonyad.viewmodel.user.UserAvailbiltyViewModel" />
    </data>
    <FrameLayout
        android:onClick="@{ () -> model.clickitem() }"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="12dp">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_marginStart="8dp"
            android:background="@{model.ischeck ? @drawable/rec_strockblue :@drawable/rec_strockblue1}"
            android:layout_gravity="end|bottom">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:text="8"
                    android:text="@{model.title}"
                    android:layout_gravity="center"
                    android:textSize="14sp"
                    android:fontFamily="@font/cairoregular"
                    android:textColor="@color/darkblack"/>

            </LinearLayout>
        </FrameLayout>
        <ImageView
            android:visibility="@{model.ischeck ? View.VISIBLE : View.GONE}"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:background="@drawable/ic_check345"/>
    </FrameLayout>
</layout>